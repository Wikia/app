<section class="trending-articles">
	<h2 id="trendingArticles">{{trendingArticlesHeading}}</h2>
	{{#trendingArticles}}
	<a href="{{pageUrl}}" title="{{pageTitle}}" class="related-article small">
		<img src="{{blankImg}}" data-src="{{imgUrl}}" alt="{{pageTitle}}" width="{{imgWidth}}" height="{{imgHeight}}" class="lazy" />
		<div class="title">{{pageTitle}}</div>
	</a>
	{{/trendingArticles}}
</section>

