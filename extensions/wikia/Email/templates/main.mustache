<html>
<head>
	<title>Wikia</title>
</head>
<body>

<div class="main-wrapper">
	<div class="fandom-bar">
		{{# icons.FandomTopBar}}
			<img src="{{url}}" height="5" width="100%" alt="Top Bar">
		{{/ icons.FandomTopBar}}
	</div>
	<table class="header-wrap">
		<tr>
			<td></td>
			<td>
				<div class="content">
					<div class="header content-inner">
						<a href="http://www.wikia.com" class="logo">
							{{# icons.FandomLogoHeader}}<img src="{{url}}" alt="Fandom">{{/ icons.FandomLogoHeader}}
						</a>
					</div>
				</div>
			</td>
			<td></td>
		</tr>
	</table>
	<table class="body-wrap">
		<tr>
			<td></td>
			<td>
				<div class="content">
					<div class="main-content content-inner">
						{{{content}}}
					</div>
				</div>
			</td>
			<td></td>
		</tr>
	</table>
	<table class="footer-wrap">
		<tr>
			<td></td>
			<td>
				<div class="content">
					<div class="footer content-inner">
						<a href="http://www.wikia.com">
							{{# icons.FandomLogoFooter}}<img src="{{url}}" alt="Fandom">{{/ icons.FandomLogoFooter}}
						</a>
						<p class="social-links">
							<a class="facebook" href="{{socialMessages.facebook-link}}" >
								{{# icons.Facebook}}<img src="{{url}}" alt="{{socialMessages.facebook}}">{{/ icons.Facebook}}
							</a>
							<a class="twitter" href="{{socialMessages.twitter-link}}" >
								{{# icons.Twitter}}<img src="{{url}}" alt="{{socialMessages.twitter}}">{{/ icons.Twitter}}
							</a>
							<a class="reddit" href="{{socialMessages.reddit-link}}" >
								{{# icons.Reddit}}<img src="{{url}}" alt="{{socialMessages.reddit}}">{{/ icons.Reddit}}
							</a>
							<a class="youtube" href="{{socialMessages.youtube-link}}" >
								{{# icons.YouTube}}<img src="{{url}}" alt="{{socialMessages.youtube}}">{{/ icons.YouTube}}
							</a>
							<a class="instagram" href="{{socialMessages.instagram-link}}" >
								{{# icons.Instagram}}<img src="{{url}}" alt="{{socialMessages.instagram}}">{{/ icons.Instagram}}
							</a>
						</p>

						{{#footerMessages}}
							<p class="fine-print">{{{.}}}</p>
						{{/footerMessages}}

						<div class="spacer">
						</div>

						{{# footerMobileApplicationMessages}}
							<p class="fine-print">{{{.}}}</p>
						{{/ footerMobileApplicationMessages}}

						<div class="mobile-badges">
							{{# iOSBadge}}
								<a class="ios-badge" href="{{link}}">
									<img src="{{url}}" alt="{{alt}}">
								</a>
							{{/ iOSBadge}}
							{{# androidBadge}}
								<a class="android-badge" href="{{link}}">
									<img src="{{url}}" alt="{{alt}}">
								</a>
							{{/ androidBadge}}
						</div>
					</div>
				</div>
			</td>
			<td></td>
		</tr>
	</table>
</div>
</body>
</html>
