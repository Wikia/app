<p class="portable-infobox-error-info">{{info}}</p>
<ul class="portable-infobox-debug">
	{{#code}}
	<li>
		<div class="portable-infobox-debug-line{{#error}} error{{/error}}">
			<div class="portable-infobox-debug-line-number">{{line}}</div>
			<div class="portable-infobox-debug-line-code">{{codeLine}}</div>
		</div>
		{{#error}}
		<div class="portable-infobox-debug-error-massage">
			{{#error_messages}}
				<p class="portable-infobox-debug-error-massage-item">{{{message}}}</p>
			{{/error_messages}}
		</div>
		{{/error}}
	</li>
	{{/code}}
</ul>