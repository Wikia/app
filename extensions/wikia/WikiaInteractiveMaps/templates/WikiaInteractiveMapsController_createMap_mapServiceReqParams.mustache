<form class="WikiaForm create-map-form">
	<div class="input-group">
		<input type="text" id="intMapTitle" name="intMapTitle" placeholder="{{titlePlaceholder}}">
	</div>
	<img src="{{thumbnailUrl}}" alt="" id="intMapPreviewImage" class="intMapPreviewImage" width="100%" height="auto" />
	{{#orgImage}}
		<input type="hidden" id="intMapOrgImg" value="{{orgImage}}">
	{{/orgImage}}
	{{#tileSetId}}
		<input type="hidden" id="intMapTileSet" value="{{tileSetId}}">
	{{/tileSetId}}
</form>
