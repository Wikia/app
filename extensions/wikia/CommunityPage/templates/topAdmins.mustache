{{#topAdmins}}
	<h2>{{topAdminsHeaderText}}</h2>
	<ul class="reset top-admins">
		{{#admins}}
			<li>
				<div class="avatar-container">
					<a href="{{profilePage}}">{{{avatar}}}</a>
				</div>
				<div class="contributor-details">
					<a href="{{profilePage}}">{{userName}}</a>
				</div>
			</li>
		{{/admins}}
	</ul>
	{{#haveOtherAdmins}}
		{{otherAdminCount}} {{otherAdmins}}
	{{/haveOtherAdmins}}
{{/topAdmins}}
