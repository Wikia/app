{{#contributorsModule}}
	<h2>{{topContribsHeaderText}}</h2>
	<div class="user-details">
		<div class="avatar-container">
			{{{userAvatar}}}
		</div>
		<div class="rank">
			<span>{{userRank}} <small>/ {{memberCount}}</small></span>
			<span class="subtle">{{yourRankText}}</span>
		</div>
		<div class="user-contrib-count">
			<span>{{userContribCount}}</span>
			<span class="subtle">{{userContributionsText}}</span>
		</div>
	</div>
	<ul>
		{{#contributors}}
			<li>
				<div class="avatar-container">
					{{{avatar}}}
				</div>
				<div class="contributor-details">
					{{count}}. <a href="{{profilePage}}">{{userName}}</a>
					{{contributionsText}}
				</div>

			</li>
		{{/contributors}}
	</ul>
{{/contributorsModule}}
