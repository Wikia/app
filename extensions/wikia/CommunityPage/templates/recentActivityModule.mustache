{{#recentActivityModule}}
	<div class="module RecentActivityModule">
		<h2 class="activity-heading">{{activityHeading}}</h2>
		<ul>
			{{#activity}}
				<li>
					<div class="avatar-container">
						{{{userAvatar}}}
					</div>
					{{{changeMessage}}}{{editedPageTitle}}
					{{#timeAgo}}
						{{timeAgo}}
					{{/timeAgo}}
				</li>
			{{/activity}}
		</ul>
		{{{moreActivityLink}}}
	</div>
{{/recentActivityModule}}
