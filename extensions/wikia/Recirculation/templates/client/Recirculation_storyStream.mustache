<a href="{{url}}" class="mcf-card mcf-card-ns-stream" data-tracking="{{trackingLabels}}"
   data-li-type="{{liType}}"
   style="background-image: linear-gradient(to bottom, rgba(0, 0, 0, 0), #000000), url({{thumbnail}});"
   title="{{title}}">
		<span class="mcf-card-ns-stream__title">
			{{title}}
		</span>
	<div class="wds-button wds-is-squished wds-is-secondary">{{buttonLabel}}</div>
</a>
