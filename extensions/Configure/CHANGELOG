    $wgSend404Code, $wgDebugToolbar, $wgDeprecationReleaseLimit,
    $wgAggregateStatsID, $wgArticleCountMethod, $wgCachePrefix,
    $wgCopyUploadsDomains, $wgDisableUploadScriptChecks, $wgUploadStashMaxAge,
    $wgUpdateCompatibleMetadata, $wgEnableAutoRotation,
    $wgExcludeFromThumbnailPurge, $wgImageMagickIdentifyCommand,
    $wgSVGMetadataCutoff, $wgExportAllowAll, $wgQueryPageDefaultLimit,
