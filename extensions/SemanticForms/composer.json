{
	"name": "mediawiki/semantic-forms",
	"type": "mediawiki-extension",
	"description": "An extension, based around Semantic MediaWiki, that allows users to create forms for adding and editing pages that use templates to store semantic data.",
	"keywords": [
		"Semantic Forms",
		"forms",
		"SF",
		"Semantic MediaWiki",
		"Wiki",
		"MediaWiki"
	],
	"homepage": "https://www.mediawiki.org/wiki/Extension:Semantic_Forms",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Yaron Koren",
			"homepage": "http://yaronkoren.com/"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "1.*,>=1.0.1",
		"mediawiki/semantic-media-wiki": ">=1.9"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.8.x-dev"
		}
	},
	"autoload": {
		"files" : [
			"SemanticForms.php"
		]
	}
}
