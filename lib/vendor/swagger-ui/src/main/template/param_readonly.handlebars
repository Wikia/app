<td class='code'>{{name}}</td>
<td>
    {{#if isBody}}
        <textarea class='body-textarea' readonly='readonly' name='{{name}}'>{{defaultValue}}</textarea>
    {{else}}
        {{#if defaultValue}}
            {{defaultValue}}
        {{else}}
            (empty)
        {{/if}}
    {{/if}}
</td>
<td>{{{description}}}</td>
<td>{{{paramType}}}</td>
<td><span class="model-signature"></span></td>
