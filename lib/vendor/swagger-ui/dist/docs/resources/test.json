{
    "basePath": "http://www.wikia.com/",
    "swaggerVersion": "1.1",
    "apiVersion": "0.2",
    "resourcePath": "/wikia.php",
    "apis": [
        {
            "path": "",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Fetches latest activity information",
                    "nickname": "getLatestActivity",
                    "parameters": [
						{
							"name": "controller",
							"allowMultiple": false,
							"dataType": "string",
							"paramType": "query",
							"required": true,
							"defaultValue": "ActivityApi"
						},
						{
							"name": "method",
							"allowMultiple": false,
							"dataType": "string",
							"paramType": "query",
