<li {{#referenceId}}data-id="{{referenceId}}" {{/referenceId}} class="wikia-dropdown-nav-item">
	<a href="{{href}}" title="{{title}}"
		{{#trackingId}}data-tracking-id="{{trackingId}}" {{/trackingId}}
		rel="nofollow"
		{{#dataAttr}}
			data-{{key}}="{{value}}"
		{{/dataAttr}}
		{{#class}} class="{{class}}" {{/class}}
	>
		{{title}}
	</a>
</li>
