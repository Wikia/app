{
	"name": "styleguide-name-modal",
	"description": "styleguide-description-modal",
	"types": {
		"default": {
			"params" : {
				"id": {
					"templateVar" : "id",
					"type" : "string",
					"description" : "styleguide-description-modal-id"
				},
				"size": {
					"templateVar" : "size",
					"type" : "enum",
					"value" : {
						"0" : "small",
						"1" : "medium",
						"2" : "large",
						"3" : "content-size"
					},
					"description" : "styleguide-description-modal-size"
				},
				"content" : {
					"templateVar" : "content",
					"type" : "string",
					"description" : "styleguide-description-modal-content"
				},
				"classes": {
					"templateVar" : "classes",
					"type" : "string",
					"description" : "styleguide-description-modal-classes"
				},
				"title": {
					"templateVar" : "title",
					"type" : "string",
					"description" : "styleguide-description-modal-title"
				},
				"htmlTitle": {
					"templateVar" : "htmlTitle",
					"type" : "string",
					"description" : "styleguide-description-modal-html-title"
				},
				"closeText": {
					"templateVar" : "closeText",
					"type" : "string",
					"description" : "styleguide-description-modal-close-text"
				},
				"escapeToClose" : {
					"templateVar" : "escapeToClose",
					"type" : "boolean",
					"description" : "styleguide-description-modal-escape-to-close"
				},
				"altLink" : {
					"templateVar" : "altLink",
					"type" : "object",
					"objectVar" : {
						"id": "string",
						"href": "string",
						"text": "string",
						"title": "string"
					},
					"description" : "styleguide-description-modal-alt-link"
				},
				"buttons": {
					"templateVar": "buttons",
					"type" : "array",
					"description" : "styleguide-description-modal-buttons"
				}
			}
		}
	}
}
